{
  "name": "@stackpath/serverless-scripting-cli",
  "description": "Deploy to StackPath's serverless scripting platform from your local machine.",
  "version": "2.1.1",
  "author": "The StackPath Development Team <developers@stackpath.com>",
  "bin": {
    "sp-serverless": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/stackpath/serverless-scripting-cli/issues"
  },
  "dependencies": {
    "@oclif/command": "^1.5.16",
    "@oclif/config": "^1.13.2",
    "@oclif/errors": "^1.2.2",
    "@oclif/parser": "^3.8.3",
    "@oclif/plugin-help": "^2.2.0",
    "@types/node-fetch": "^2.3.7",
    "cli-ux": "^5.3.1",
    "node-fetch": "^2.6.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^7.0.2",
    "@types/node": "^10.14.12",
    "chai": "^4.2.0",
    "globby": "^10.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^9.2.0",
    "mocha": "^10.2.0",
    "nock": "^12.0.3",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.3.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "bin",
    "lib",
    "CODE_OF_CONDUCT.md",
    "npm-shrinkwrap.json",
    "oclif.manifest.json"
  ],
  "homepage": "https://github.com/stackpath/serverless-scripting-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sp-serverless",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackpath/serverless-scripting-cli.git"
  },
  "scripts": {
    "generate:doc": "typedoc --out dist/docs src/ --mode modules",
    "lint": "tslint -p . -c tslint.json 'src/**/*.ts'",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && rm -rf tsconfig.tsbuildinfo && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
